<service>
	<id>gymflowsyncserver.exe</id>
	<name>GymFlow Sync Server</name>
	<description>Socket.IO sync server for GymFlow real-time workout sessions.</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>C:\xampp\htdocs\Training\sync-server\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\xampp\htdocs\Training\sync-server\server.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>GymFlow Sync Server wrapper</argument>
